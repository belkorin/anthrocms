
{
  "version": "2.4.2",
  "compilerOptions": {
      "lib": ["es5", "es6"],
      "target": "es6",
      "module": "CommonJS",
      "moduleResolution": "node",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true
  },
  "exclude": [
      "node_modules"
  ]
}
